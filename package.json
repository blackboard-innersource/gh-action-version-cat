{
  "name": "github-action",
  "version": "0.0.0",
  "private": true,
  "description": "GitHub Action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blackboard-innersource/gh-action-version-cat.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "blackboard-innersource",
  "dependencies": {
    "@actions/core": "^1.2.3",
    "@actions/exec": "^1.0.3",
    "@actions/io": "^1.0.2"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/node": "^13.13.14",
    "@typescript-eslint/parser": "^2.34.0",
    "@zeit/ncc": "^0.21.1",
    "eslint": "^6.8.0",
    "eslint-plugin-github": "^3.4.1",
    "eslint-plugin-jest": "^23.18.0",
    "jest": "^25.5.4",
    "jest-circus": "^25.5.4",
    "js-yaml": "^3.14.0",
    "prettier": "^1.19.1",
    "ts-jest": "^25.5.1",
    "typescript": "^3.9.7"
  }
}
